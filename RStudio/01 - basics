> #numerico
> val <- 100
> val
[1] 100
> #caracteres
> string <- "Olá Mundo!"
> string
[1] "Olá Mundo!"
> #Lógicos
> 10 > 8
[1] TRUE
> #Numeros complexos
> num <- 5 + 7i
> num
[1] 5+7i
> 
> mode(val)
[1] "numeric"
> length(val)
[1] 1
> mode(string)
[1] "character"
> length(string)
[1] 1
> mode(10>8)
[1] "logical"
> length(10>8)
[1] 1
> mode(num)
[1] "complex"
> length(num)
[1] 1